{% extends 'base.html.twig' %}

{% block title %}Hello FillDBController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

    {% if alreadyHaveData %}
        <h1>Les données sont déjà dans la DB! ✅</h1>
    {% else %}
    <h1>Les données ont été enregistrés dans la DB! ✅</h1>
    {% endif %}

</div>
{% endblock %}
